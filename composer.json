{
  "name": "ngvritishvili/open_ai_cli",
  "description": "integrate open ai with php for cli support",
  "type": "laravel-package",
  "license": "MIT",
  "authors": [
    {
      "name": "nikoloz Gvritishvili",
      "email": "ngvritishvili@hotmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "guzzlehttp/guzzle": "^7.2",
    "laravel/framework": "^10.10",
    "laravel/prompts": "^0.1.13",
    "laravel/sanctum": "^3.3",
    "laravel/tinker": "^2.8",
    "openai-php/laravel": "^0.8.1"
  },
  "autoload": {
    "psr-4": {
      "AI\\": "src/AI",
      "Config\\": "src/config",
      "Console\\Commands\\": "src/Console/Commands/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
