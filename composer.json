{
  "name": "ngvritishvili/open_ai_cli",
  "version": "1.0.0",
  "description": "Integrate OpenAI with PHP for CLI support",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "cli",
    "laravel",
    "chatgpt",
    "open-ai",
    "openai",
    "integration"
  ],
  "authors": [
    {
      "name": "Nikoloz Gvritishvili",
      "email": "ngvritishvili@hotmail.com"
    }
  ],
  "homepage": "https://github.com/ngvritishvili/open_ai_cli",
  "require": {
    "php": "^8.0",
    "guzzlehttp/guzzle": "^7.2",
    "laravel/sanctum": "^3.3",
    "laravel/tinker": "^2.8",
    "openai-php/laravel": "^0.8.1",
    "laravel/prompts": "^0.1.14"
  },
  "require-dev": {
    "laravel/framework": "^10",
    "phpunit/phpunit": "^9.4|^10.1"
  },
  "autoload": {
    "psr-4": {
      "AI\\": "src/",
      "Config\\": "src/Config/",
      "Console\\": "src/Console/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "AI\\AIPackageServiceProvider"
      ]
    },
    "branch-alias": {
      "dev-main": "1.0.0-dev",
      "1.0.0": "1.0.0-dev"
    }
  }
}
